{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "test": {
      "dependsOn": ["build"],
      "cache": false
    },
    "build": {
      "dependsOn": ["^build", "generate"],
      "outputs": ["build/**"]
    },
    "generate": {
      "dependsOn": ["^build"],
      "outputs": ["src/gen/**", "gen/**"]
    }
  }
}
